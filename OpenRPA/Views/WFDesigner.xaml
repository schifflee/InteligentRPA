<UserControl x:Class="OpenRPA.Views.WFDesigner"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OpenRPA.Views"
             mc:Ignorable="d" Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded" KeyUp="UserControl_KeyUp"
             d:DesignHeight="450" d:DesignWidth="800" Padding="0" Margin="0,0,0,0">
    <ContentControl Content="{Binding WorkflowDesigner.View}" Padding="0" Margin="0,0,0,0" />
</UserControl>
